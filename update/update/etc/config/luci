
config core 'main'
	option resourcebase '/luci-static/resources'
	option mediaurlbase '/luci-static/bootstrap'
	option lang 'zh_cn'

config extern 'flash_keep'
	option uci '/etc/config/'
	option dropbear '/etc/dropbear/'
	option openvpn '/etc/openvpn/'
	option passwd '/etc/passwd'
	option opkg '/etc/opkg.conf'
	option firewall '/etc/firewall.user'
	option uploads '/lib/uci/upload/'

config internal 'languages'
	option zh_cn 'chinese'

config internal 'sauth'
	option sessionpath '/tmp/luci-sessions'
	option sessiontime '3600'

config internal 'ccache'
	option enable '1'

config internal 'themes'
	option Bootstrap '/luci-static/bootstrap'
	
config command
	option name '翻墙方案一：带pdnsd'
	option command 'sh /etc/config/autowall'
	
	
config command
	option name '翻墙方案二：no-pdnsd'
	option command 'sh /etc/config/autowall-no-pdnsd'
	
config command
	option name '一键设置(luci)'
	option command 'sh /etc/config/quicksetup'
	
config command
	option name '恢复固件出厂设置'
	option command 'sh /etc/config/firstboot'
	
config command
	option name '增强无线设置'
	option command 'sh /etc/config/mwifi'	
	
config command
	option name '多拨设置'
	option command 'sh /etc/config/mppoe'
	
config command
	option name '无线信道13(联网)'
	option command 'sh /etc/config/13channel'
	
config command
	option name 'IPTV设置(lan4)'
	option command 'sh /etc/config/miptv'
	
config command
	option name '安装迅雷(要联网)'
	option command 'sh /etc/config/mxunlei'	
	
config command
	option name '在线更新固件'
	option command 'sh /etc/config/mupdate'


config command
	option name '重启'
	option command 'reboot'
	
config command
	option name 'ssh修复'
	option command 'sh /etc/config/ssh-fix'
	
config command
	option name '还原设置'
	option command 'sh /etc/config/restore-backup'
	
config command
	option name '快速设置(自用)'
	option command 'sh /mnt/sda2/tmp/quick-setup'

